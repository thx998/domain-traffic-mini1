.wrap{
  font-size: 14px;
  color: #333;
  padding: 12px 12px 0 12px;
  background-color: #f5f5f5;
}
.h1{
  font-size: 17px;
  font-weight: 600;
}
.h2{
  font-weight: 500;
}
.text{
  font-size: 14px;
  padding-bottom: 110px;
}
.warn{
    color: #f96e63;
}

.bottom{
  box-sizing: border-box;
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  background-color: #f5f5f5;
  padding: 10px 16px;
}
.agree{
  margin-bottom: 10px;
  background-color: #FF594A !important;
}
.disagree{
  color: #D2D2D2 !important;
}
.agree, .disagree{
  width: 100% !important;
}