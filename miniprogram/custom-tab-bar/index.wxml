<van-tabbar active="{{ active }}" bind:change="onChange" active-color='#FF5B5C' inactive-color='#6E7580' fixed="{{true}}">
  <van-tabbar-item info="">
    <image
      slot="icon"
      src="{{ list[0].icon.normal }}"
      mode="aspectFit"
      style="width: 48rpx; height: 48rpx;"
    />
    <image
      slot="icon-active"
      src="{{ list[0].icon.active }}"
      mode="aspectFit"
      style="width: 48rpx; height: 48rpx;"
    />
   卡包
  </van-tabbar-item>
  <van-tabbar-item info="">
    <image
      slot="icon"
      src="{{ list[1].icon.normal }}"
      mode="aspectFit"
      style="width: 48rpx; height: 48rpx;"
    />
    <image
      slot="icon-active"
      src="{{ list[1].icon.active }}"
      mode="aspectFit"
      style="width: 48rpx; height: 48rpx;"
    />
    我的
  </van-tabbar-item>
</van-tabbar>
